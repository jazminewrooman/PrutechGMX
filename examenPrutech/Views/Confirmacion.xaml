<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="GMX.Views.Confirmacion">
	<ContentPage.Content>
        <StackLayout>
            <StackLayout x:Name="slEnBanco">
            </StackLayout>
            <StackLayout x:Name="slTarjeta">
            </StackLayout>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="50*"/>
                    <ColumnDefinition Width="50*"/>
                </Grid.ColumnDefinitions>
                <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" x:Name="lblSehaenviado"/>
                <Label Grid.Row="1" Grid.Column="0" Text="Prima Total:"/>
                <Label Grid.Row="2" Grid.Column="0" Text="Número de póliza:"/>
                <Label Grid.Row="1" Grid.Column="1" x:Name="lblprima" />
                <Label Grid.Row="2" Grid.Column="1" x:Name="lblpoliza" />
            </Grid>
        </StackLayout>
	</ContentPage.Content>
</ContentPage>
